{
  "name": "fs-app-template",
  "version": "2.0.0",
  "description": "boilerplate code (no auth)",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "build:dev": "npm run build -- --watch --mode=development",
    "seed": "node script/seed.js",
    "start": "node server",
    "start:dev": "npm run build:dev & npm run start-server",
    "start:dev:logger": "LOGGING=true npm run start:dev",
    "start:dev:seed": "SEED=true npm run start:dev",
    "start-server": "JWT=shh nodemon server -e html,js,scss --ignore public --ignore client",
    "test": "NODE_ENV=test JWT=a_secret mocha \"./server/**/*.spec.js\" \"./client/**/*.spec.js\" --require @babel/register --require esm ",
    "test:dev": "npm run test -- --watch",
    "test:dev:models": "npm run test:dev -- --grep=model",
    "test:dev:routes": "npm run test:dev -- --grep=route"
  },
  "author": "Fullstack Academy of Code",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.3.4",
    "bcrypt": "^5.1.0",
    "compression": "^1.7.4",
    "css-loader": "^6.7.3",
    "css-modules": "^0.3.0",
    "express": "^4.18.2",
    "history": "^5.3.0",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "node-polyfill-webpack-plugin": "^2.0.1",
    "pg": "^8.10.0",
    "react-router": "^6.10.0",
    "sequelize": "^6.30.0",
    "style-loader": "^3.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/preset-react": "^7.18.6",
    "@babel/register": "^7.21.0",
    "@testing-library/react": "^14.0.0",
    "axios": "^1.3.4",
    "axios-mock-adapter": "^1.21.4",
    "babel-loader": "^9.1.2",
    "chai": "^4.3.7",
    "enzyme": "^3.11.0",
    "esm": "^3.2.25",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.22",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.10.0",
    "react-test-renderer": "^18.2.0",
    "redux": "^4.2.1",
    "redux-devtools-extension": "^2.13.5",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.4",
    "redux-thunk": "^2.4.2",
    "supertest": "^6.3.3",
    "webpack": "^5.77.0",
    "webpack-cli": "^5.0.1"
  }
}
